            <string>{
    platform_collisions_create(100);

    //Constants

    fight = true
    attack_interval = 10
    attack_a_interval = 15
    attack_phase = false
    full_uppercut = -1
    upcut_ready_interval = 10
    upcut_charge_interval = 30
    upcut_swipe_interval = 1
    upcut_swipe_part = object_upcut_swipe_part_l
    slide = false
    no_of_aerial_attacks = 2
    player_damage_range = 24
    wall_jump_speed = 6
    wall_jump_interval = 15
    hurt_interval = 20
    hurt_disable = 18
    h_slide_speed = 0
    // stun
    stunned = "none"
    stun_speed = 0
    shield_contact = true
    invulnerable_interval = 40
    sleepy = false
    if (!sleepy) {
        run_speed = 5
        footsteps_interval = 8
    } else {
        run_speed = 2
        footsteps_interval = 20
    }
    disable = false

    //Collisions
    wall_jump = false
    wall_grind_start = false
    
    //Movement
    // Hspeed is reset after contacting sloped service, thus slope_speed
    // is used as a temporary store for previous step speed checking
    slope_speed = 0
    jump = false
    D = "rig"

    //Timers

    wall_l_jump_timer = -1;
    wall_r_jump_timer = -1;

    attack_timer = -1
    attack_a_timer = -1;
    
    upcut_ready_timer = -1
    upcut_charge_timer = -1
    upcut_swipe_timer = -1
    
    footsteps_timer = -1
    
    invulnerable_timer = -1
    
    // Music
    //audio_play_sound(spyro, 6, false)

    //Attacking
    attack = false
    uppercut = false
    attack_animation = 2
    moe_damage = 10
    attack_range = 16
    // Detects if player has run out of attacks after executing aerial attacks
    attacks_left = false
    // Aerial
    aerial_attacks_completed = 0
    

    //Actions
    action = false
    item_gain = false

    //Animation
    moe_run_r = moe_run_right
    moe_run_l = moe_run_left
    moe_upcut_ready = moe_uppercut_ready_left
    moe_upcut_charge = moe_uppercut_charge_left
    moe_upcut_charged = moe_uppercut_charged_left
    moe_upcut_swipe_1 = moe_uppercut_swipe_1_l
    moe_upcut_swipe_2 = moe_uppercut_swipe_2_l
    moe_upcut_swipe_3 = moe_uppercut_swipe_3_l
        
    animation_change = true
    // Freeze animation during room transition
    room_transition_freeze = false
    freeze_sprite = ""
    freeze_image = 0
    freeze_step = 0

    //Initialize attack animations array
    attack_ani[0] = moe_attack_wood_sword_right_1
    attack_ani[1] = moe_attack_wood_sword_left_1
    attack_ani[2] = moe_attack_wood_sword_right_2
    attack_ani[3] = moe_attack_wood_sword_left_2
    attack_ani[4] = moe_attack_wood_sword_right_3
    attack_ani[5] = moe_attack_wood_sword_left_3
    attack_ani[6] = moe_attack_aerial_wood_sword_right_1
    attack_ani[7] = moe_attack_aerial_wood_sword_left_1
    attack_ani[8] = moe_attack_aerial_wood_sword_right_2
    attack_ani[9] = moe_attack_aerial_wood_sword_left_2

    //Health
    moe_health = 100
    global.player_health = moe_health

    //Damage
    damage_object_pos[0] = 0
    damage_object_pos[1] = 0
    hurt_timer = -1
    player_recoil = 6
    
    texture_set_interpolation(false);
    
}
