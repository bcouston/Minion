    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>{

    // Bugs to fix

    // --- ANIMATION ---
    
    // Idle/Fight Animation
    if animation_change == true {
        if (sleepy == true) {
            // Sleepy physics
            run_speed = 2
            footsteps_interval = 20
            //
            moe_walk_r = moe_sleepy_still_right
            moe_walk_l = moe_sleepy_still_left
    
            moe_run_r = moe_sleepy_walk_right
            moe_run_l = moe_sleepy_walk_left
    
            moe_jump_r = moe_jump_right
            moe_jump_l = moe_jump_left
            moe_jump_descent_r = moe_jump_descent_right
            moe_jump_descent_l = moe_jump_descent_left
    
            moe_wall_grab_r = moe_wall_grab_right
            moe_wall_grab_l = moe_wall_grab_left  
        }
        else if (fight == false)  {
            moe_walk_r = moe_walk_right
            moe_walk_l = moe_walk_left
    
            moe_run_r = moe_run_right
            moe_run_l = moe_run_left
    
            moe_jump_r = moe_jump_right
            moe_jump_l = moe_jump_left
            moe_jump_descent_r = moe_jump_descent_right
            moe_jump_descent_l = moe_jump_descent_left
    
            moe_wall_grab_r = moe_wall_grab_right
            moe_wall_grab_l = moe_wall_grab_left
        } else {
            // Normal physics
            run_speed = 5
            footsteps_interval = 8
            //
            moe_walk_r = moe_walk_sword_right
            moe_walk_l = moe_walk_sword_left
            //moe_walk_r = moe_sleepy_still_right
            //moe_walk_l = moe_sleepy_still_left
    
            moe_run_r = moe_run_right_wood_sword
            moe_run_l = moe_run_left_wood_sword
            //moe_run_r = moe_sleepy_walk_right
            //moe_run_l = moe_sleepy_walk_left
    
            moe_jump_r = moe_jump_right_wood_sword
            moe_jump_l = moe_jump_left_wood_sword
            moe_jump_descent_r = moe_jump_descent_right
            moe_jump_descent_l = moe_jump_descent_left
    
            moe_wall_grab_r = moe_wall_grab_right_wood_sword
            moe_wall_grab_l = moe_wall_grab_left_wood_sword
            
            moe_attack_ready_r = moe_attack_ready_right
            moe_attack_ready_l = moe_attack_ready_left
            
        }
        animation_change = false
    }
    
    // Item gain
    if (item_gain) {
        sprite_index = moe_find_item
        if (image_index == image_number - 1) {
            image_speed = 0
        }
    }
    // Player death
    else if (moe_health &lt;= 0) {
        instance_create(x,y,object_moe_death_part)
        // Audio
        audio_stop_sound(wind_fall)
        audio_stop_sound(wall_grind)
        //
        instance_destroy()
    }
    // if player is in midst of damage
    else if (hurt_timer != -1) {
        if (hurt_timer == 0) {
            hurt_timer = -1;
        } else {
            hurt_timer -= 1;
            gravity = 0.8
            if (hurt_timer &gt; hurt_disable) {
                speed = player_recoil
                player_direction = point_direction(x,y,damage_object_pos[0],damage_object_pos[1])
                direction = (player_direction + 180)
            }
            if (direction &gt;= 90) &amp;&amp; (direction &lt;= 270) {
                sprite_index = moe_damage_r
            } else { sprite_index = moe_damage_l}
        }
    }
    // if player is shield stunned
    else if (stunned == "left") {
        sprite_index = moe_stunned_r
        //y = y - 8
        hspeed = 8
        vspeed = -4
        stun_speed = 0.05
        stunned = "active left"
        // Cancel uppercut
        if uppercut {
            uppercut = false
            h_slide_speed = 0
            upcut_ready_timer = -1
            upcut_charge_timer = -1
            upcut_swipe_timer = -1
            audio_play_sound(swing_cancel, 10, false)
            audio_stop_sound(upcut_charge)
        }
    } else if (stunned == "right") {
        sprite_index = moe_stunned_l
        //y = y - 8
        hspeed = -8
        vspeed = -4
        stun_speed = -0.05
        stunned = "active right"
        // Cancel uppercut
        if uppercut {
            uppercut = false
            h_slide_speed = 0
            upcut_ready_timer = -1
            upcut_charge_timer = -1
            upcut_swipe_timer = -1
            audio_play_sound(swing_cancel, 10, false)
            audio_stop_sound(upcut_charge)
        }
    }
    // If attacking and not wall jump
    if (attack_a_timer != -1 &amp;&amp; place_free(x,y+1) &amp;&amp; wall_jump == false) {
        gravity = 0
        vspeed = 0
        hspeed = 0
    // If attacking and wall jump
    } else if (attack == true &amp;&amp; attack_a_timer != -1 &amp;&amp; place_free(x,y+1) &amp;&amp; wall_jump == true) {
        gravity = 0.8
    // If attacking on ground
    } else if (attack == true &amp;&amp; attack_timer != -1 &amp;&amp; !place_free(x,y+1)) {
        gravity = 0
        hspeed = 0
    }
    // If not touching ground
    else if place_free(x,y+1) {
        gravity = 0.8;
        //Room transition handle
        if (global.room_exit == false) {
            jump = true
        }
        footsteps_timer = -1
        //Sprite change if not stunned
        if (stunned == "none") {
            if D == "lef" {
                if place_free(x-1,y) {
                    if vspeed &lt;= 0 {
                        sprite_index = moe_jump_l
                    } else {sprite_index = moe_jump_descent_l}
                }
            } else if D == "rig" {
                if place_free(x+1,y) {
                    if vspeed &lt;= 0 {
                        sprite_index = moe_jump_r
                    } else {sprite_index = moe_jump_descent_r}
                }
            }
        }
        // If touching wall
        if (place_meeting(x+2,y,object_solid) || place_meeting(x-2,y,object_solid)) {
            //Sprite change if not stunned
            if (stunned == "none") {
                if place_meeting(x+2,y,object_solid) {
                    sprite_index = moe_wall_grab_r
                } else if place_meeting(x-2,y,object_solid) {
                    sprite_index = moe_wall_grab_l
                }
                if (vspeed &gt; 0) {
                    gravity = 0.1;
                    // Wall grind
                    if !(wall_grind_start) {
                        // Audio
                        audio_play_sound(wall_grind, 1, false)
                        wall_grind_start = true
                    }
                    // Particles
                    if D == "rig" {
                        instance_create(bbox_right, bbox_bottom, object_wall_grind_dust_part)
                    } else if D == "lef" {
                        instance_create(bbox_left, bbox_bottom, object_wall_grind_dust_part)
                    }
                    // Stop fall sound
                    audio_stop_sound(wind_fall)
                }
            }
        // If aerial and not touching wall
        } else {
            wall_grind_start = false
            audio_stop_sound(wall_grind)
            // Falling sound
            if (vspeed &gt; 0) {
                if (!audio_is_playing(wind_fall)) {
                    audio_play_sound(wind_fall, 1, false)
                }
            }
        }
    } else {
        // If on top of shield but the shield is not facing upwards
        shield = instance_place(x,y+2,object_solid_dummy_shield)
        if (shield == noone) {
            shield = instance_place(x,y+2,object_solid_dummy_perma_shield)
        } 
        if (shield != noone) {
            shield_contact = true
            if (shield.shield_orient != "up") {
                gravity = 0.0;
                if (shield.shield_orient == "right") { 
                    hspeed = 8
                } else {
                    hspeed = -8
                }
            }
        // If touching ground
        } if (shield == noone) || (shield.shield_orient == "up") {
            shield_contact = false
            gravity = 0;
            // Attack
            aerial_attacks_completed = 0
            attacks_left = true
            // Jumping
            if (jump) {
                //Audio
                audio_sound_pitch(floor_impact, random_range(1.0,1.2))
                audio_play_sound(floor_impact, 1, false)
                //Particles
                instance_create(x, bbox_bottom, object_land_dust_part)
            }
            // Increase friction if stunned and sliding on ground
            if (stunned == "active left") {
                stun_speed = 0.2
                stunned = "ground slide"   
            } else if (stunned == "active right") {
                stun_speed = -0.2
                stunned = "ground slide"
            }
            jump = false
            wall_jump = false
            // Wall grind
            wall_grind_start = false
            audio_stop_sound(wall_grind)
            // Stop fall sound
            audio_stop_sound(wind_fall)
            // if player is moving
            if (stunned == "none") {
                if (hspeed != 0) &amp;&amp; (h_slide_speed == 0) {
                    if (hspeed &gt; 0) {
                        sprite_index = moe_run_r
                    } else if (hspeed &lt; 0) {
                        sprite_index = moe_run_l
                    }
                    if footsteps_timer == -1 {
                        footsteps_timer = footsteps_interval
                    }
                }
                // If player is still
                else if D == "rig" &amp;&amp; hspeed == 0 &amp;&amp; vspeed == 0 &amp;&amp; attack == false &amp;&amp; !uppercut {
                    if (!attack_phase) {
                        sprite_index = moe_walk_r
                    } else {
                        sprite_index = moe_attack_ready_r
                    }
                    footsteps_timer = -1;
                }
                else if D == "lef" &amp;&amp; hspeed == 0 &amp;&amp; vspeed == 0 &amp;&amp; attack == false &amp;&amp; !uppercut {
                    if (!attack_phase) {
                        sprite_index = moe_walk_l
                    } else {
                        sprite_index = moe_attack_ready_l
                    }
                    footsteps_timer = -1
                }
            }
        }
    }
    
    // descent animation stays around too long when attacking as soon as you hit ground
    if (!global.player_disable) { // Player disable (for cutscenes etc.)
    
        // Horizontal speed control
        if hspeed &gt; 12 {
            hspeed = 12
        }
        
        // --- SOUND ---
    
    
        //--- DAMAGE ---
        damage = false
    
        // Animation for jumping freeze
        if((sprite_index == moe_jump_r || sprite_index == moe_jump_l) &amp;&amp; (image_index == image_number - 1) ) {
            image_speed = 0
        } else {image_speed = 1}
    
        // Animation for attack ceasing
        if ( (attack_timer &lt;= attack_interval - 1) &amp;&amp; attack == true) {
             if (image_index == image_number - 1) {
                image_speed = 0
             }
             // Attack timer has depleted and on last frame of attack animation
             if (attack_timer == -1 &amp;&amp; !uppercut &amp;&amp; image_index == image_number - 1) {
                attack = false
                image_speed = 1
             }
        }
        
        // --- Timers ---
    
        // Wall jump speed
    
        if (wall_l_jump_timer &lt;= 0) {
          wall_l_jump_timer = -1;
        } else {
          wall_l_jump_timer -= 1;
          hspeed = wall_jump_speed
        }
        if (wall_r_jump_timer &lt;= 0) {
          wall_r_jump_timer = -1;
        } else {
          wall_r_jump_timer -= 1;
          hspeed = -1 * wall_jump_speed 
        }
    
        // Puts Moe in an "attack phase" while he is still attacking within a certain interval
        if (attack_timer &lt;= 0) {
            attack_timer = -1;
        } else {
            attack_timer -= 1;
        }
    
        // Aerial attack freeze
        if (attack_a_timer == 0) {
            attack_a_timer = -1;
            image_speed = 1
        } else if (attack_a_timer != -1) {
            attack_a_timer -= 1;
            if (image_index == image_number - 1) {
                image_speed = 0
            }
        }
        
        // Uppercut
        
        // Uppercut ready timer
        
        if (upcut_ready_timer == 0) {
            upcut_ready_timer = -2;
            sprite_index = moe_upcut_charge
            audio_play_sound(upcut_charge, 10, false)
            upcut_charge_timer = upcut_charge_interval;
        } else if (upcut_ready_timer &gt; 0) {
            upcut_ready_timer -= 1;
        }
        
        // Uppercut charge timer
        
        if (upcut_charge_timer == 0) {
            upcut_charge_timer = -2;
            sprite_index = moe_upcut_charged
        } else if (upcut_charge_timer &gt; 0) {
            upcut_charge_timer -= 1;
            if (upcut_charge_timer &lt;= 20) {
                // Build up number of frames swipe animation takes
                if (upcut_swipe_interval &lt; 20) {
                    upcut_swipe_interval += 1
                }
            }
            if (image_index == image_number - 1) {
                image_speed = 0
            }
        }
        
        // Uppercut swipe timer
        
        if (upcut_swipe_timer == 0) {
            upcut_swipe_timer = -2;
            sprite_index = moe_upcut_swipe_3
            image_index = 0
        } else if (upcut_swipe_timer &gt; 0) {
            upcut_swipe_timer -= 1;
            if (upcut_swipe_timer mod 2 == 0) {
                instance_create(x, bbox_bottom, upcut_swipe_part)
            }
        }
        
        // Keep track of how many aerial attacks have elapsed
        if (aerial_attacks_completed == no_of_aerial_attacks) {
            attacks_left = false;
        }
            
        //Wall jump collision
        if (!place_free(x+1,y) || !place_free(x-1,y)) {
            wall_jump = false
            aerial_attacks_completed = 0
            attacks_left = true
        }
    
        // --- Gravity when attacking in air, when attacking on ground, when jumping, in contact with a wall and on the floor, with animations ---
    
        // Found wood sword in chest
        //if (object_chest_wood_sword.open) {
        //    gravity = 0
        //    hspeed = 0
        //    vspeed = 0
        //   sprite_index = moe_find_wood_sword
        //}
        // Moe is hurt
    
        // L/R movement and jumping
        if keyboard_check(ord('D')) &amp;&amp; !keyboard_check(ord('A')) &amp;&amp; attack == false &amp;&amp; !uppercut &amp;&amp; (hurt_timer &lt;= hurt_disable)
             {
             D = "rig"
                                    // Additional check for sloped surface
             if place_free(x+1,y) || place_meeting(x+1,y+1,object_dungeon_slope_right) || place_meeting(x-1,y+1,object_dungeon_slope_left) {
                // Air movement
                if place_free(x,y+1) &amp;&amp; !shield_contact {
                    if hspeed &lt;= run_speed {
                        hspeed += 1
                    }
                // Ground movement
                } else if !shield_contact {
                    hspeed = run_speed;
                }
            } else if !place_free(x+1,y) &amp;&amp; !place_free(x,y+1) {
                sprite_index = moe_walk_r
            }
            // Cancel attack ready stance
            attack_phase = false;
        }
    
        if keyboard_check(ord('A')) &amp;&amp; !keyboard_check(ord('D')) &amp;&amp; attack == false &amp;&amp; !uppercut &amp;&amp; (hurt_timer &lt;= hurt_disable)
             {
             D = "lef"
                                    // Additional check for sloped surface
             if place_free(x-1,y) || place_meeting(x-1,y+1,object_dungeon_slope_left) || place_meeting(x+1,y+1,object_dungeon_slope_right) {
                 // Air movement
                if place_free(x,y+1) &amp;&amp; !shield_contact {
                    if hspeed &gt;= -1 * run_speed {
                        hspeed -= 1;
                    }
                // Ground movement
                } else if !shield_contact {
                    hspeed = -1 * run_speed;
                }
            } else if !place_free(x-1,y) &amp;&amp; !place_free(x,y+1) {
                sprite_index = moe_walk_l
            }
            // Cancel attack ready stance
            attack_phase = false;
        }
        
        /// DEBUGGING -------------------------------------
        
        // Teleport left or right in case of collision bug
        if keyboard_check_pressed(ord('K')) {
            x -= 10
        }
        
        if keyboard_check_pressed(ord('L')) {
            x += 10
        }
        
        // ------------------------------------------------
        
        if (!sleepy) { //Sleepy
    
            // Jump
            if keyboard_check_pressed(ord('W')) &amp;&amp; attack == false &amp;&amp; !uppercut {
                // Wall jump
                if place_free(x,y+1) &amp;&amp; place_free(x,y-1) {
                    if place_meeting(x+5,y,object_solid) { // Some tolerance for proximity to the wall
                        vspeed = -10
                        hspeed = -1 * wall_jump_speed
                        D = "lef"
                        wall_jump = true
                        jump = true
                        wall_r_jump_timer = wall_jump_interval
                        //Audio
                        audio_sound_pitch(moe_jump, random_range(0.8,1.2))
                        audio_play_sound(moe_jump, 5, false)
                    }
                    if place_meeting(x-5,y,object_solid) {
                        vspeed = -10
                        hspeed = wall_jump_speed
                        D = "rig"
                        wall_jump = true
                        jump = true
                        wall_l_jump_timer = wall_jump_interval
                        //Audio
                        audio_sound_pitch(moe_jump, random_range(0.8,1.2))
                        audio_play_sound(moe_jump, 5, false)
                    }
                    // Normal jump
                } else {
                    if !place_free(x,y+1) &amp;&amp; place_free(x,y-1) &amp;&amp; !shield_contact {
                        vspeed = -10
                        jump = true
                        //Audio
                        audio_sound_pitch(moe_jump, random_range(0.8,1.2))
                        audio_play_sound(moe_jump, 5, false)
                    }
                }
            // Analog jump - method sourced from info.sonicretro.org/SPG:Jumping
            } else if keyboard_check_released(ord('W')) &amp;&amp; attack == false &amp;&amp; !uppercut {
                if place_free(x,y+1) &amp;&amp; vspeed &lt; -7 {
                    vspeed = -7
                }
            }
        
            // Sword attack
        
            if fight {
        
                // Right
                if keyboard_check_pressed(vk_right) {
                    if (attack_timer &lt;= 1 &amp;&amp; attack == false &amp;&amp; !uppercut &amp;&amp; attacks_left == true) {
                        D = "rig"
                        attack = true
                        damage = true
                        moe_damage = 10
                        // Reset image speed if not already
                        image_speed = 1
                        // Spawn combat invisible hitbox if not facing shield
                        if (instance_position( x + attack_range + 12, y, object_dummy_shield_collision)!= noone) {
                            audio_sound_pitch(shield_blocked, random_range(0.8,1.2))
                            audio_play_sound(shield_blocked, 20, false)
                            with (instance_nearest(x, y, object_solid_dummy_shield)) {
                                sprite_index = dummy_shield_block
                            }
                        } else {
                            instance_create(x + attack_range, y, object_player_combat_hitbox)
                        }
                        attack_timer = attack_interval
                        attack_phase = true
                        attack_animation++
                        // Audio
                        audio_sound_pitch(swoosh_1, random_range(0.9,1.1))
                        audio_play_sound(swoosh_1, 10, false)
                        audio_stop_sound(wind_fall)
                        if (aerial_attacks_completed != no_of_aerial_attacks &amp;&amp; place_free(x,y+1)) {
                            attack_a_timer = attack_a_interval
                            aerial_attacks_completed++
                        }
                        //if attack_a_timer != -1 {
                            if (attack_animation mod 2 == 0) {
                                if !(place_free(x,y+1)) {
                                    sprite_index = attack_ani[0]
                                } else if (attack_a_timer != -1) {sprite_index = attack_ani[6]}
                            } else if  (attack_animation mod 2 == 1) {
                                if !(place_free(x,y+1)) {
                                    sprite_index = attack_ani[2]
                                } else if (attack_a_timer != -1) {sprite_index = attack_ani[8]}
                            } else {
                                if !(place_free(x,y+1)) {
                                    sprite_index = attack_ani[4]
                                } else if (attack_a_timer != -1) {sprite_index = attack_ani[6]}
                            }
                        //}
                    }
                }
        
                // Left
                if keyboard_check_pressed(vk_left) {
                    if (attack_timer &lt;= 1 &amp;&amp; attack == false &amp;&amp; !uppercut &amp;&amp; attacks_left == true) {
                        D = "lef"
                        attack = true
                        damage = true
                        moe_damage = 10
                        // Reset image speed if not already
                        image_speed = 1
                        // Spawn combat invisible hitbox if not facing shield
                        if (instance_position( x - attack_range - 12, y, object_dummy_shield_collision) != noone) {
                            audio_sound_pitch(shield_blocked, random_range(0.8,1.2))
                            audio_play_sound(shield_blocked, 20, false)
                            with (instance_nearest(x, y, object_solid_dummy_shield)) {
                                sprite_index = dummy_shield_block
                            }
                        } else {
                            instance_create(x - attack_range, y, object_player_combat_hitbox)
                        }
                        attack_timer = attack_interval
                        attack_phase = true
                        attack_animation++
                        // Audio
                        audio_sound_pitch(swoosh_1, random_range(0.9,1.1))
                        audio_play_sound(swoosh_1, 10, false)
                        audio_stop_sound(wind_fall)
                        if (aerial_attacks_completed != no_of_aerial_attacks &amp;&amp; place_free(x,y+1)) {
                            attack_a_timer = attack_a_interval
                            aerial_attacks_completed++
                        }
                        //if attack_a_timer != -1 {
                            if (attack_animation mod 2 == 0) {
                                if !(place_free(x,y+1)) {
                                    sprite_index = attack_ani[1]
                                } else if (attack_a_timer != -1) {sprite_index = attack_ani[7]}
                            } else if (attack_animation mod 2 == 1) {
                                if !(place_free(x,y+1)) {
                                    sprite_index = attack_ani[3]
                                } else if (attack_a_timer != -1) {sprite_index = attack_ani[9]}
                            } else {
                                if !(place_free(x,y+1)) {
                                    sprite_index = attack_ani[5]
                                } else if (attack_a_timer != -1){sprite_index = attack_ani[7]}
                            }
                        //}
                    }
                    
                }
        
                // Up
                if keyboard_check_pressed(vk_up) {
                    if (attack_timer &lt;= 1 &amp;&amp; upcut_ready_timer &lt;= -1 &amp;&amp; upcut_charge_timer &lt;= -1 &amp;&amp; upcut_swipe_timer &lt;= -1 &amp;&amp; !place_free(x,y+1) &amp;&amp; place_free(x,y-1) &amp;&amp; !uppercut) {
                        upcut_ready_timer = upcut_ready_interval
                        upcut_swipe_interval = 1
                        uppercut = true
                        attack_phase = true
                        footsteps_timer = -1
                        //damage = true
                        //attack_timer = attack_interval
                        if (D == "lef") {
                            moe_upcut_ready = moe_uppercut_ready_left
                            moe_upcut_charge = moe_uppercut_charge_left
                            moe_upcut_charged = moe_uppercut_charged_left
                            moe_upcut_swipe_1 = moe_uppercut_swipe_1_l
                            moe_upcut_swipe_2 = moe_uppercut_swipe_2_l
                            moe_upcut_swipe_3 = moe_uppercut_swipe_3_l
                            upcut_swipe_part = object_upcut_swipe_part_l
                            h_slide_speed = 0.15
                        } else if (D == "rig") {
                            moe_upcut_ready = moe_uppercut_ready_right
                            moe_upcut_charge = moe_uppercut_charge_right
                            moe_upcut_charged = moe_uppercut_charged_right
                            moe_upcut_swipe_1 = moe_uppercut_swipe_1_r
                            moe_upcut_swipe_2 = moe_uppercut_swipe_2_r
                            moe_upcut_swipe_3 = moe_uppercut_swipe_3_r
                            upcut_swipe_part = object_upcut_swipe_part_r
                            h_slide_speed = -0.15
                        }
                        sprite_index = moe_upcut_ready
                        audio_play_sound(upcut_ready, 10, false)
                    }
                }
            
                //If Down key has been pressed to cancel uppercut
                if keyboard_check_pressed(vk_down) &amp;&amp; uppercut {
                    uppercut = false
                    h_slide_speed = 0
                    upcut_ready_timer = -1
                    upcut_charge_timer = -1
                    upcut_swipe_timer = -1
                    audio_play_sound(swing_cancel, 10, false)
                    audio_stop_sound(upcut_charge)    
                }
                
            // If Up key has been released
                if keyboard_check_released(vk_up) &amp;&amp; uppercut {
                    // if uppercut has achieved full charge
                    if (upcut_ready_timer == -2) {
                        upcut_charge_timer = -1 
                        sprite_index = moe_upcut_swipe_1
                        image_index = 0
                        audio_stop_sound(upcut_charge)
                        audio_sound_pitch(swoosh_1, random_range(0.7,0.9))
                        audio_play_sound(swoosh_1, 10, false)
                    } else {
                        uppercut = false
                        h_slide_speed = 0
                        upcut_ready_timer = -1
                        audio_play_sound(swing_cancel, 10, false)
                        audio_stop_sound(upcut_charge)
                    }
                }
                
                // If uppercut has achieved full charge
                if (upcut_ready_timer == -2) {
                    // If swipe animation 1 is complete go onto swipe animation 2
                    if ((sprite_index == moe_upcut_swipe_1) &amp;&amp; (image_index == image_number - 1)) {
                        sprite_index = moe_upcut_swipe_2
                        audio_play_sound(upcut_swipe_1, 1, false)
                        upcut_swipe_timer = upcut_swipe_interval    
                    }
                    
                    // If swipe timer for animation 2 is complete go onto swipe animation 3
                    if (upcut_swipe_timer == -2) {
                        upcut_swipe_timer = -1
                        if (D == "lef") {
                            instance_create(x - attack_range, y, object_player_combat_hitbox)
                        } else if (D == "rig") {
                            instance_create(x + attack_range, y, object_player_combat_hitbox)
                        }
                        audio_stop_sound(upcut_swipe_1)
                        // fully charged
                        if (upcut_swipe_interval == 20) {
                            moe_damage = 20
                            // fully charged uppercut - break shield if near by
                            full_uppercut = 1
                            audio_play_sound(upcut_swipe_2, 1, false)
                        } else {
                            moe_damage = 10 + (0.5 * upcut_swipe_interval)
                            // partially charged uppercut - stun enemy
                            full_uppercut = 0
                            audio_sound_pitch(swoosh_1, random_range(0.8,1.0))
                            audio_play_sound(swoosh_1, 10, false)
                        }
                    }
                    
                    // Reset at the end of swipe 3 animation
                    if ((sprite_index == moe_upcut_swipe_3) &amp;&amp; (image_index == image_number - 1)) {
                            uppercut = false
                            full_uppercut = -1
                            upcut_ready_timer = -1
                            upcut_charge_timer = -1
                            upcut_swipe_timer = -1
                    }
                    
                }
            }
            
        } //Sleepy
    
        if keyboard_check_pressed(vk_space) {
            action = true
        } else {action = false}
    
        //Damage
        
        // Collision detection with damage object
        if (invulnerable_timer == -1) {
            damage_hitbox_inst = instance_position(bbox_left,y,object_player_damage_hitbox)
            if (damage_hitbox_inst == noone) {
                damage_hitbox_inst = instance_position(bbox_right,y,object_player_damage_hitbox)
            }
            if (damage_hitbox_inst == noone) {
                damage_hitbox_inst = instance_position(x,bbox_top,object_player_damage_hitbox)
            }
            if (damage_hitbox_inst == noone) {
                damage_hitbox_inst = instance_position(x,bbox_bottom,object_player_damage_hitbox)
            }
            if (damage_hitbox_inst != noone) {
                if (hurt_timer &lt; 0) {
                    hurt_timer = hurt_interval
                    damage_object_pos[0] = damage_hitbox_inst.x
                    damage_object_pos[1] = damage_hitbox_inst.y
                    player_recoil = damage_hitbox_inst.player_recoil
                    moe_health -=  damage_hitbox_inst.player_damage
                    global.player_health = moe_health
                    //Audio
                    audio_sound_pitch(moe_hurt, random_range(0.9,1.1))
                    audio_play_sound(moe_hurt, 1, false)
                    audio_stop_sound(wall_grind)
                }
            }
        }
        
        //Invulnerability after spawn animation
        if (invulnerable_timer != -1) {
            invulnerable_timer--
            if visible {
                visible = false
            } else {visible = true}
        } else {visible = true}
        
        // Static friction
        if ((!keyboard_check(ord('D'))) &amp;&amp; (!keyboard_check(ord('A')))) {
            platform_friction(1,true);
        } else {
            platform_friction(1,false);
        }
        
        // Horizontal slide decelerate
        
        if (h_slide_speed != 0) {
            if !slide {
                slide = true
                audio_sound_pitch(ground_slide, random_range(0.9,1.1))
                audio_play_sound(ground_slide, 1, false)    
            }
            if ((hspeed &lt;= 0.05) &amp;&amp; (hspeed &gt;= -0.05))  || (!place_free(x - (h_slide_speed * 40), y)) {
                h_slide_speed = 0
                hspeed = 0
                slide = false
            }
            hspeed += h_slide_speed
            if (hspeed &gt; 0) {
                instance_create(x - 4, bbox_bottom, h_slide_part_r)
            } else {instance_create(x + 4, bbox_bottom, h_slide_part_l)}
            
        }
        
    } // Player disable (for cutscenes etc.)
    
    // Disable collisions in cutscenes but enable when player is stunned
    
    if (!global.player_disable) || (stunned != "none") {
        // Collisions
        platform_collisions_step(object_solid)
        platform_collisions_step(object_solid_non_wj)
        platform_collisions_step(object_solid_dummy_shield)
        platform_collisions_step(object_solid_dummy_perma_shield)
        //platform_collisions_step(object_dungeon_ground)
        //platform_collisions_step(object_dungeon_stairs)
        //platform_collisions_step(object_dungeon_slant)
        platform_collisions_step(object_dungeon_slope_right)
        platform_collisions_step(object_dungeon_slope_left)
        //platform_collisions_step(object_large_button)
        //platform_collisions_step(object_gate)
        //platform_collisions_step(object_shield_dummy1)
        //platform_collisions_step(object_knight_boss)
    }
    
    // Stun decelerate     
    if (stun_speed != 0) {
        if (hspeed == 0) || (!place_free(x + hspeed, y)) {
            stun_speed = 0
            hspeed = 0
            stunned = "none"
            global.player_disable = false
        } else {
            hspeed -= stun_speed
        }
    }
    
    // Footsteps
    if (footsteps_timer == 0) {
        footsteps_timer = footsteps_interval
    } else if (footsteps_timer &gt; 0) {
        if (footsteps_timer == footsteps_interval) {
            //Audio
            audio_sound_pitch(footstep, random_range(1.0,1.4))
            audio_play_sound(footstep, 1, false)
            //Particles
            instance_create(x, bbox_bottom, object_walk_dust_part)
        }
        footsteps_timer -= 1
    }
    
    //if (room_get_name(room) == "room_dungeon_overworld") {
    //    sleepy = false
    //}
    
    // Disable (e.g. during cutscene) but not stunned
    if (global.player_disable) &amp;&amp; (stunned == "none") {
        if !disable {
            hspeed = 0
            disable = true
        }
    } else {disable = false}
    
    // Freeze (during room transition)
    if (room_transition_freeze) {
        image_speed = 0
        hspeed = 0
        vspeed = 0
        gravity = 0
        audio_stop_sound(wind_fall)
        // Set
        if (freeze_step == 0) {
            freeze_sprite = sprite_get_name(sprite_index)
            freeze_index = image_index
        }
        sprite_index = asset_get_index(freeze_sprite)
        image_index = freeze_index
        freeze_step++
    } else {freeze_step = 0}
    
}
</string>
